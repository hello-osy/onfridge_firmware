{
  "configurations": [
    {
      "name": "PlatformIO",
      "includePath": [
        "${workspaceFolder}/include",
        "${workspaceFolder}/src",
        "${workspaceFolder}/esp32/**",
        "${workspaceFolder}/lib/**",
        "/esp-idf/components/**",
        ""
      ],
      "browse": {
        "limitSymbolsToIncludedHeaders": true,
        "path": [
          "${workspaceFolder}/include",
          "${workspaceFolder}/src",
          "/esp-idf/components/**",
          "${workspaceFolder}/esp32/**",
          "${workspaceFolder}/lib/**",
          ""
        ]
      },
      "defines": [
        "_POSIX_READER_WRITER_LOCKS",
        "_GNU_SOURCE",
        "_GLIBCXX_USE_POSIX_SEMAPHORE",
        "_GLIBCXX_HAVE_POSIX_SEMAPHORE",
        "SOC_XTAL_FREQ_MHZ=CONFIG_XTAL_FREQ",
        "SOC_MMU_PAGE_SIZE=CONFIG_MMU_PAGE_SIZE",
        "IDF_VER=\"5.3.1\"",
        "ESP_PLATFORM",
        "PLATFORMIO=60116",
        "ARDUINO_ESP32_DEV",
        ""
      ],
      "cStandard": "gnu17",
      "cppStandard": "gnu++23",
      "compilerPath": "/esp-idf/tools/xtensa-esp32-elf/bin/xtensa-esp32-elf-gcc",
      "compilerArgs": ["-mlongcalls", ""]
    }
  ],
  "version": 4
}
